{
  "name": "trips",
  "version": "1.0.0",
  "description": "Trips app",
  "main": "index.js",
  "scripts": {
    "stats": "node_modules/.bin/webpack --env stats --profile --json > stats.json",
    "start": "npm run start:client && npm run start:server",
    "start:server": "node_modules/.bin/nodemon",
    "start:server-debug": "node --inspect ./index.js",
    "start:client": "node_modules/.bin/webpack-dev-server --port=3000 --inline --env dev --progress --history-api-fallback --open --colors",
    "test": "node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanluke/trips.git"
  },
  "keywords": [
    "Trips"
  ],
  "author": "Lukasz Gonciarz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanluke/trips/issues"
  },
  "engines": {
    "node": ">= 7.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "globals": {
      "__TEST__": true
    },
    "projects": [
      "<rootDir>",
      "<rootDir>/server/*",
      "<rootDir>/client/*"
    ]
  },
  "homepage": "https://github.com/vanluke/trips#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.0",
